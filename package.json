{
  "name": "eq-grid",
  "version": "0.1.1",
  "description": "Next generation grid made via custom element",
  "main": "eq-grid.min.js",
  "dependencies": {},
  "devDependencies": {
    "git-branch-is": "^3.1.0",
    "husky": "^4.2.5",
    "uglify-es": "^3.3.9"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "uglify": "uglifyjs eq-grid.js -o eq-grid.min.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run uglify",
      "postmerge": "(git-branch-is master && npm version minor || (git-branch-is dev && npm --no-git-tag-version version patch)"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rezi/eq-grid.git"
  },
